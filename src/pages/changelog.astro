---
import Docs from "layouts/docs.astro";
import marked from "lib/marked";
import { readFile } from "fs/promises";

const e = await readFile("md/changelog.md", "utf-8");
const m = await marked(e);
---

<Docs
  doc={{
    title: "Changelog",
    meta: {
      robots: "noindex",
    },
  }}
  tbc={[]}
  slug="changelog"
>
  <Fragment set:html={m.html} />
</Docs>
